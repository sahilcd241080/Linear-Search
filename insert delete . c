#include<stdio.h>
#include<stdlib.h>

void insert(int arr[],int *n,int pos,int key){
    for(int i=*n;i>pos;i--){
        arr[i] = arr[i - 1];
    }
    arr[pos]=key;
    (*n)++;
}

void deleted(int arr[],int *n,int pos){
    if(pos<0 || pos>=*n){
        printf("invailed index number");
        return;
    }
    for(int i=pos;i<*n - 1;i++){
        arr[i]=arr[i+1];
    }
    (*n)--;
}

int main(){
    int arr[20] = {9,3,5,2,6,7,1,4};
    int n = 8;
    int pos = 5;
    int key = 45;
    //insert(arr,&n,pos,key);
   // deleted(arr,&n,pos);
    for(int i=0;i<n;i++){
        printf("%d  ",arr[i]);
    }
    return 0;
}

